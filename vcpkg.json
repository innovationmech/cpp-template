{
  "name": "cpp-template",
  "version": "1.0.0",
  "description": "Modern C++ Project Template",
  "homepage": "https://github.com/example/cpp-template",
  "license": "MIT",
  "builtin-baseline": "897ba2ab4c4c776b985ab1f599548fcf3ae598ba",
  "dependencies": [
    "gtest",
    "fmt",
    "nlohmann-json",
    {
      "name": "cpprestsdk",
      "platform": "!uwp"
    }
  ],
  "features": {
    "networking": {
      "description": "Enable networking capabilities",
      "dependencies": [
        "curl",
        "openssl"
      ]
    },
    "database": {
      "description": "Enable database support",
      "dependencies": [
        "sqlite3",
        "postgresql"
      ]
    }
  },
  "overrides": [
    {
      "name": "fmt",
      "version": "10.1.1"
    }
  ]
}
